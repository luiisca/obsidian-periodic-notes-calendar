{
  "name": "periodic-notes-calendar",
  "version": "1.1.1",
  "description": "Obsidian plugin to organize periodic notes from daily to yearly with a calendar interface. Navigate and summarize entries using stickers in the calendar view.",
  "author": "luiisca",
  "main": "main.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --mode development --watch",
    "build": "npm run lint && vite build --mode production",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold error",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold error --watch",
    "format": "biome format --write ./src",
    "biome-lint": "npm run check && biome lint --apply-unsafe ./src",
    "lint": "eslint ./src",
    "lint-fix": "npm run check && eslint --fix ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.39.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.10.4",
    "autoprefixer": "^10.4.20",
    "builtin-modules": "^4.0.0",
    "chrono-node": "^2.7.7",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "globals": "^16.5.0",
    "obsidian": "latest",
    "postcss": "^8.4.47",
    "svelte": "^5.1.10",
    "svelte-check": "^4.0.5",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.4.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@floating-ui/dom": "^1.5.1",
    "clsx": "^2.0.0",
    "emoji-mart": "^5.6.0",
    "emoji-regex": "^10.4.0",
    "svelte-writable-derived": "^3.1.1",
    "tailwind-merge": "^2.5.4",
    "uuid": "^11.0.3"
  }
}

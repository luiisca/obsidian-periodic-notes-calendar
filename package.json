{
    "name": "periodic-notes-calendar",
    "version": "1.0.0",
    "description": "Obsidian plugin for creating and accessing daily notes for any date. Use command palette shortcuts or a calendar UI to navigate. Features emoji summaries for days, weeks, months, and years",
    "author": "luiisca",
    "main": "main.js",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dev": "rollup -c -w --environment NODE_ENV:prod",
        "check": "svelte-check --tsconfig ./tsconfig.json --threshold error",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold error --watch",
        "format": "prettier --plugin-search-dir . --write .",
        "lint": "npm run check && eslint .",
        "build": "npm run lint && rollup -c"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@rollup/plugin-typescript": "^11.1.3",
        "@skeletonlabs/tw-plugin": "^0.1.0",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@types/node": "^20.10.4",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.30.0",
        "npm-run-all": "^4.1.5",
        "obsidian": "latest",
        "postcss": "^8.4.45",
        "postcss-cli": "^11.0.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.10.1",
        "rollup": "^3.28.1",
        "rollup-plugin-svelte": "^7.2.2",
        "sass": "^1.69.5",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^5.1.1",
        "tailwindcss": "^3.4.11",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.4.2"
    },
    "dependencies": {
        "@emoji-mart/data": "^1.2.1",
        "@floating-ui/dom": "^1.5.1",
        "clsx": "^2.0.0",
        "emoji-mart": "^5.6.0",
        "moment": "^2.29.4"
    }
}
